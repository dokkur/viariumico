nav.via-nav
  ul.via-nav-menu
    - var navItems = [{title:'About', link:'/about'}, {title:'WP', link:'/wp'}, {title:'Documents', link:'/documents'}, {title:'Team', link:'/team'}, {title:'Blog', link:'/blog'}, {title:'Contacts', link:'/contacts'}, {title:'OnePage', link:'/onepage'}];
    each navItem, index in navItems
      if index === 2
        li.via-nav-menu-item.-active
          a.via-nav-menu-item-link.-active(href=navItem.link)= navItem.title
      else
        li.via-nav-menu-item
          a.via-nav-menu-item-link(href=navItem.link)= navItem.title